{
  "name": "web3-multicall3",
  "version": "0.2.0-beta.1",
  "scripts": {
    "build": "concurrently --kill-others-on-fail \"yarn:build:*\"",
    "build:esm": "tsc --build tsconfig.cjs.json && echo '{\"type\": \"commonjs\"}' > ./dist/cjs/package.json",
    "build:cjs": "tsc --build tsconfig.esm.json && echo '{\"type\": \"module\"}' > ./dist/esm/package.json",
    "build:types": "tsc --build tsconfig.types.json",
    "build:web": "webpack",
    "test": "jest"
  },
  "description": "Type-safe typescript wrapper for multicall3",
  "keywords": [
    "typescript",
    "type-safe",
    "web3",
    "multicall3",
    "multicall",
    "browser",
    "esm",
    "commonjs"
  ],
  "author": {
    "name": "Demid Mosko",
    "email": "demid.mosko@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/djomaa/web3-multicall3.git"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "browser": "dist/web3/index.min.js",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.js",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./plugin": {
      "types": "./dist/types/plugin/index.d.js",
      "import": "./dist/esm/plugin/index.js",
      "require": "./dist/cjs/plugin/index.js"
    }
  },
  "peerDependencies": {
    "web3": ">= 4.0.2 < 5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "concurrently": "^8.2.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "web3": "^4.1.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
